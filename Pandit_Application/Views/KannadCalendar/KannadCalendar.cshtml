        }*/

        .event-list {
            list-style: none;
            /*font-family: 'Lato', sans-serif;*/
            margin: 0px;
            padding: 2px;
        }

            .event-list > li {
                background-color: rgb(165, 82, 167);
                /*box-shadow: 0px 0px 5px rgb(51, 51, 51);*/
                box-shadow: 2px 2px 3px rgba(51, 51, 51, 0.7);
                padding: 0px;
                margin: 5px 0px 1px;
            }

                .event-list > li > time {
                    display: inline-block;
                    width: 100%;
                    height: 50%;
                    color: rgb(255, 255, 255);
                    background-color: #ff7e00;
                    text-align: center;
                    text-transform: capitalize;
                }

                .event-list > li:nth-child(even) > time {
                    background-color: #ff7e00;
                }

                .event-list > li > time > span {
                    display: none;
                }

                .event-list > li > time > .date {
                    display: block;
                    font-size: 20pt;
                    font-weight: 100;
                    line-height: 1.5;
                }

                .event-list > li time > .day {
                    display: block;
                    font-size: 12pt;
                    font-weight: 900;
                    line-height: 1;
                    text-transform: capitalize
                }


                .event-list > li > .info {
                    padding-top: 15px;
                    text-align: center;
                }

                    .event-list > li > .info > .title {
                        font-size: 12pt;
                        font-weight: 700;
                        margin: 0px;
                        text-transform: capitalize;
                    }

                    .event-list > li > .info > .desc {
                        font-size: 13pt;
                        font-weight: 300;
                        margin: 0px;
                    }

                    .event-list > li > .info > ul {
                        display: table;
                        list-style: none;
                        margin: 10px 0px 0px;
                        padding: 0px;
                        width: 100%;
                        text-align: center;
                    }

                        .event-list > li > .info > ul > li {
                            display: table-cell;
                            cursor: pointer;
                            color: rgb(30, 30, 30);
                            font-size: 11pt;
                            font-weight: 300;
                            padding: 3px 0px;
                        }

                            .event-list > li > .info > ul > li > a {
                                display: block;
                                width: 100%;
                                color: rgb(30, 30, 30);
                                text-decoration: none;
                            }




                .event-list > li > time {
                    display: inline-block;
                }

                .event-list > li > time {
                    width: 50px;
                    float: left;
                }

                .event-list > li > .info {
                    background-color: rgb(245, 245, 245);
                    overflow: hidden;
                }

                .event-list > li > time {
                    width: 100px;
                    height: 70px;
                    padding: 0px;
                    margin: 0px;
                }

                .event-list > li > .info {
                    position: relative;
                    height: 70px;
                    text-align: left;
                    padding-right: 40px;
                }

                    .event-list > li > .info > .title,
                    .event-list > li > .info > .desc {
                        padding: 0px 10px;
                    }

                    .event-list > li > .info > ul {
                        position: absolute;
                        left: 0px;
                        bottom: 0px;
                    }


        .hs_about_progress_cont_left .accordionFifteen .panel .panel-heading a.collapsed {
            background-color: transparent;
        }

        .hs_about_progress_cont_left .accordionFifteen .panel .panel-heading a {
            background-color: #ff7e00;
        }
    </style>
