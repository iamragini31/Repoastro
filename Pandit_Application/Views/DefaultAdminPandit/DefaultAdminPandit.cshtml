                            <h3 class="text-danger number">Rejected Orders</h3>
                            <p class="stat-text " style="font-size:20px;">@Html.DisplayFor(model => model.rejectedservice)</p>
                        </div>
                    </a>
                </div>
            </div>


        </div>


        <div class="statistics" style="display:none">
