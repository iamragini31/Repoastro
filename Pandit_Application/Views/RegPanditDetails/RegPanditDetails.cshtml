                                                                <th>Price (INR)</th>                                                            
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @*<tr>
                                                                <td>1</td>
                                                                <td>Satyanarayan</td>
                                                                <td>44</td>
                                                                <td>12</td>
                                                               
                                                            </tr>*@

                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="tab-pane" id="completed_tasks">
                                            <div class="card-body">
                                                <div class="table-responsive">
                                                    <table class="table table-hover mb-0" id="tblPujas">
                                                        <thead>
                                                            <tr>
                                                                <th>Serial Number</th>
                                                                <th>Puja Names</th>
                                                                <th>Price (USD)</th>
                                                                <th>Price (INR)</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @*<tr>
                                                                <td>12</td>

                                                                <td>Ganesh Puja</td>
                                                                <td>44</td>
                                                                <td>12</td>

                                                            </tr>*@

