                                                        <li>
                                                            <div class="title">Country: </div>
                                                            <div class="text">@Html.DisplayFor(model => model.Country, new { @id = "Country", }).</div>
                                                        </li>
                                                        <li>
                                                            <div class="title">Pin/Zip Code: </div>
                                                            <div class="text">@Html.DisplayFor(model => model.Pin, new { @id = "Pin", }).</div>
                                                        </li>
                                                    </ul>

                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-6 d-flex">
