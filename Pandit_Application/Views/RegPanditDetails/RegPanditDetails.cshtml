                                <li class="breadcrumb-item active">Profile</li>
                            </ul>
                        </div>
                    </div>
                </div>
                <!-- /Page Header -->

                <div class="card mb-0">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="profile-view">
                                    <div class="profile-img-wrap">
                                        <div class="profile-img">
                                            <a href="#"><img class="" src="" id="ProfileImage"></a>
                                        </div>
                                    </div>
                                    <div class="profile-basic">
                                        <div class="row">
                                            <div class="col-md-5">
                                                <div class="profile-info-left">
                                                    <h3 class="user-name m-t-0 mb-0">@Html.DisplayFor(model => model.Fullname, new { @id = "Fullname", })</h3>
                                                    @*<h6 class="text-muted">Palmistry, tarot.</h6>*@
                                                    <small class="text-muted">Palmistry, tarot.</small>
                                                    <div class="staff-id">Years Of Experience: @Html.DisplayFor(model => model.YearsOfExperience, new { @id = "YearsOfExperience", }) years</div>
                                                    <div class="small doj text-muted">Date of Join : 1st Jan 2013</div>
                                                    @*<div class="staff-msg"><a class="btn btn-custom" onclick="deleteItem();" href="chat.html">Validation done</a></div>*@
                                                </div>
                                            </div>
                                            <div class="col-md-7">
                                                <ul class="personal-info">
                                                    <li>
                                                        <div class="title">Phone:</div>
                                                        <div class="text"><a href="">@Html.DisplayFor(model => model.PhoneNumber, new { @id = "PhoneNumber", })</a></div>
                                                    </li>
                                                    <li>
                                                        <div class="title">Email:</div>
                                                        <div class="text">@Html.DisplayFor(model => model.EmailAddress, new { @id = "EmailAddress", })</div>
                                                    </li>
                                                    <li>
                                                        <div class="title">Gender:</div>
                                                        <div class="text">@Html.DisplayFor(model => model.Gender, new { @id = "Gender", })</div>
                                                    </li>
                                                    <li>
                                                        <div class="title">Date of Birth:</div>
                                                        <div class="text">@Html.DisplayFor(model => model.DateofBirth, new { @id = "DateofBirth", })</div>
                                                    </li>
                                                    <li>
                                                        <div class="title">Languages:</div>
                                                        <div class="text">@Html.DisplayFor(model => model.Languages, new { @id = "Languages", })</div>
                                                    </li>

                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="pro-edit"><a data-target="#profile_info" data-toggle="modal" class="edit-icon" href="#"><i class="fa fa-pencil"></i></a></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

