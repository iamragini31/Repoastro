                                        <h4 style="text-align:center;color:#1f1f1f">Call and chat rates</h4>
                                        <table class="table table-hover mb-0" id="CallChat">
                                            <thead>
                                                <tr>
                                                    <th>Call Rate (USD)</th>
                                                    <th>Call Rate (INR)</th>
                                                    <th>Call Rate (USD)</th>
                                                    <th>Chat Rate (INR)</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @*<tr>
                        <td>1</td>
                        <td>Satyanarayan</td>
                        <td>44</td>
                        <td>12</td>

                    </tr>*@

                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                                <div class="project-task">
                                    <ul class="nav nav-tabs nav-tabs-top nav-justified mb-0">
                                        <li class="nav-item"><a class="nav-link active" href="#all_tasks" data-toggle="tab" aria-expanded="true">Reports Rates.</a></li>
                                        <li class="nav-item"><a class="nav-link" href="#pending_tasks" data-toggle="tab" aria-expanded="false">Services Rates</a></li>
                                        <li class="nav-item"><a class="nav-link" href="#completed_tasks" data-toggle="tab" aria-expanded="false">Pujas Rate</a></li>
                                    </ul>
                                    <div class="tab-content">
                                        <div class="tab-pane show active" id="all_tasks">
                                            <div class="card-body">
                                                <div class="table-responsive">
                                                    <table class="table table-hover mb-0" id="tblreports">
                                                        <thead>
                                                            <tr>

                                                                <th>Serial Number</th>
                                                                <th>Report Name</th>
                                                                <th>Report (USD)</th>
                                                                <th>Report (INR)</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @*<tr>
                                                                <td>34</td>
                                                                <td>44</td>
                                                                <td>12</td>
                                                                <td>18</td>
                                                                <td>700</td>
                                                                <td>70</td>
                                                            </tr>*@
                                                           
