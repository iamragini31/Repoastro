@*testgit*@

<script>
    debugger
    const searchParams = new URLSearchParams(window.location.search);
    var fivemincharge = searchParams.get("fivemincharge");


    function AddMoney(amt) {
        debugger
        var n = amt.toFixed(2);
        var walletamt = $("#hdnwallet").val();
        var n1 = fivemincharge * 1;
        var n2 = n1.toFixed(2);

        var ReqdAmt = n2 - n;

        if (n < n1) {
            alert("Plaese add $" + ReqdAmt + " more for atleat 5 minute of talktime")
        }
        else {
            $.ajax({
                url: "/AddWalletMoney/RazorpayGenerateOrderid?amount=" + amt + "",
                type: "GET",
                contentType: "application/json",
                dataType: "json",
                success: function (response) {
                    if (response != null || response != "") {

                        window.location.href = "/PrePayment/PrePayment_Call_Chat"
                        //window.location.reload();
                    }
                    else {

                        alert("Error Occured");


                    }
                },
                error: function (err) {

                    alert("Error Occured");
                }
            });
        }
    }
</script>

